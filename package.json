{
  "name": "node-sample",
  "version": "0.0.0-development",
  "description": "A sample Node.js app using Express 4",
  "main": "index.js",
  "scripts": {
    "start": "node index.js",
    "commit": "git-cz",
    "semantic-release": "semantic-release"
  },
  "dependencies": {
    "express": "^4.13.3",
    "registry-url": "^5.1.0"
  },
  "engines": {
